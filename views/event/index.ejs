<div class="row">
    <% events.forEach((model) => { %>
    <div class="card col-6 p-4">
        <a href="/event/view/<%= model.id %>" class="card-title"><%= model.name %></a>
        <div class="row">
            <img src="<%= model.img_url %>" class="col-4" height="300vh">
            <p class="col-8"><%= model.short_desc %></p>        
        </div>
    </div>

<% }); %>
</div>